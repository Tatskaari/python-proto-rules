[Plugin "python"]
Target = //plugins:python

[Plugin "proto"]
Target = //plugins:proto
ProtocTool = //third_party/proto:protoc
LanguageDef = //build_defs:python

[Plugin "python_proto"]
ProtoPlugin = //protoc_plugin:plugin

[PluginDefinition]
Name = python_proto

[PluginConfig "plugin"]
ConfigKey = ProtoPlugin
DefaultValue = /home/jpoole/.local/bin/protoc-gen-python_grpc
Inherit = true

[PluginConfig "proto_dep"]
ConfigKey = ProtoDep
DefaultValue = //third_party/python:protobuf
Inherit = true

[PluginConfig "grpc_dep"]
ConfigKey = GrpcDep
DefaultValue = //third_party/python:grpclib
Inherit = true
